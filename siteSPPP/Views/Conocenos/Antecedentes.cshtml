@model IEnumerable<siteSPPP.Models.PLANTILLA>
@{
    ViewBag.Title = "Antecedentes";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@foreach (var item in Model)
{
    <dl class="dl-horizontal">

        <dd>
            <span style="font-size:1.5em;text-align:justify">
                <strong>
                    @Html.DisplayFor(model => item.TITULO)
                </strong>
            </span>
        </dd>
        <hr />

        <dd class="text-left">
            @Html.Raw(item.CONTENIDO)
        </dd>

    </dl>

}


